&lt;script&gt;     
var images = [&quot;images/J2.jpg&quot;, &quot;images/J2O.JPG&quot;]
var imgState = 0;
var imgTag = document.getElementById(&quot;imgClickAndChange&quot;);

imgTag.addEventListener(&quot;click&quot;, function (event) {
  imgState = (++imgState % images.length);
